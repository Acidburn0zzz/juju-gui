<ul class="unstyled">
    {{#notifications}}
  <li id="{{clientId}}" class="notice">
    <div{{#if seen}} class="seen" {{/if}} class="{{level}}">
      <h4>{{title}}</h4>
      <div>
        {{message}}
      </div>
      {{#if link}}
      <a href="{{link}}"><i class="icon-arrow-right"></i>{{link_title}}</a>
      {{/if}}
      <small data-timestamp="{{timestamp}}" class="timestamp">{{humanizeTime timestamp}}</small>
    </div>
  </li>
  {{/notifications}}
</ul>
