<div class="menu">
    <ul>
        {{#relationslist endpoints}}
        <li
            data-startservice="{{start.service}}"
            data-startname="{{start.name}}"
            data-endservice="{{end.service}}"
            data-endname="{{end.name}}">
            {{start.service}}:{{start.name}} &rarr; {{end.service}}:{{end.name}}
        </li>
        {{/relationslist}}
    </ul>

    <ul>
      {{#each networks}}
        <li>
          {{one}} &rarr; {{two}}
        </li>
      {{/each}}
    </ul>
    <button class="cancel button">Cancel</button>
</div>
