{{#networks}}
  <div class="settings-wrapper">
   <label for="{{name}}">{{title}}</label>
    <div>
      {{name}}
      <input class="network-field {{name}}" type="checkbox" name="{{name}}"
          value="{{value}}" data-bind="networks.{{name}}" />
      {{#if unit }}<span class="text-in-input">{{unit}}</span>{{/if}}
    </div>
    {{>conflict-ux}}
  </div>
{{/networks}}
