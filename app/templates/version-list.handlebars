<div class="unit-list-header">
  <div class="change-version-close link">
    <i class="sprite close-inspector-click"></i>
  </div>
  Change version
</div>
<ul class="unit-list">
  {{#each versions}}
  <li>
    <a href="{{strip_cs this}}">{{this}}</a>
    <a class="upgrade-link right-link" data-upgradeto="{{this}}">Upgrade</a>
  </li>
  {{/each}}
</ul>
