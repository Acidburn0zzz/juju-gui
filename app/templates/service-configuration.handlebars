<div class="view-container settings-config">
  <h2>Expose</h2>
  <div class="toggle settings-wrapper">
    <span>Expose this service?</span>
    <div class="toggle-switch toggle-expose" data-bind="exposed">
      <input type="checkbox" class="hidden-checkbox expose-toggle" id="expose-toggle">
      <label for="expose-toggle" class="light">
        <div class="handle"></div>
      </label>
    </div>
  </div>

  <h2>Service settings</h2>
  <div class="config-file">
    <input type="file">
    <div class="fakebutton">Import config file...</div>
  </div>
  {{> service-configuration}}
</div>
