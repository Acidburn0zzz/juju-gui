        "config-flags": "None", 
        "overwrite": "true"
      }, 
      "constraints": {
        "ubuntu-series": "precise", 
        "provider-type": "maas"
      }
    }, 
    {
      "name": "keystone", 
      "exposed": false, 
      "unit_count": 1, 
      "charm": "cs:precise/keystone-14", 
      "config": {
        "admin-user": "admin", 
        "admin-token": "ubuntutesting", 
        "openstack-origin": "distro", 
        "verbose": "false", 
        "database": "keystone", 
        "admin-role": "Admin", 
        "admin-port": 35357, 
        "service-port": 5000, 
        "keystone-service-admin-role": "KeystoneServiceAdmin", 
        "config-file": "/etc/keystone/keystone.conf", 
        "token-expiry": "2017-02-05T00:00", 
        "keystone-admin-role": "Admin", 
        "service-tenant": "services", 
        "debug": "false", 
        "database-user": "keystone", 
        "admin-password": "openstack"
