{
  "name": "Juju GUI",
  "manifest_version": 2,
  "version": "0.2.0",

  "description": "Manage your Juju environment",
  "icons": {
    "128": "app/assets/images/juju-128.png"
  },

  "app": {
    "urls": [
      "*://{{server.public_hostname}}/"
    ],
    "launch": {
      "web_url": "http://{{server.public_hostname}}:{{server.port}}/"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
